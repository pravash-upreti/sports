    "graphql": "^14.3.1",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/express": "^4.17.0",
    "@types/graphql": "^14.2.1",
    "@types/graphql-depth-limit": "^1.1.0",
    "@types/node": "^12.0.8",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^0.7.1",
    "graphql-import-node": "^0.0.4",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
